.tourGuide.pb-80.makeMyTrip__block
	.blockCanCollapse__title
		h2.makeMyTripOptions__sectionTitle Tour guide
		span.fal.fa-chevron-down
	.blockCanCollapse__content
		p Following your information, we shortlist these available tour guides. You can search more here.
		.formTourSearch.hotelSearch
			.options__search.d-flex.flex-wrap
				.keyword
					input(type="text" placeholder="Where are you searching for?")
				.datePicker(date-picker)
					input.datePicker__input(type="text" autocomplete="off" data-date-range )
					label.datePicker__label
						span.fal.fa-calendar-alt.datePicker__icon
						span.label.datePicker__text Start
						img.datePicker__arrow(src="./assets/icons/arrow_long.svg")
						span.label.datePicker__text End
				.select
					select
						option English
						option English
						option English
						option English
				//- .select
				//- 	select
				//- 		option 1 seats
				//- 		option 2 seats
				//- 		option 3 seats
				//- 		option 4 seats
			.button
				button.btn.btn__submit--reverse Search
		.formTourSearch__resultText.hotelSearch__resultText
			p
				b.formTourSearch__note 24 transportations 
				| meet your criterias
			p.formTourSearch__clearResult
				span.text Clear search
				span.fas.fa-times
		.formTourSearch__result
			.formTourSearchItem.shadow--1
				label.formTourSearchItem__check
					input.d-none(type="checkbox")
					span.formTourSearchItem__check--icon
				.formTourSearchItem__wrapper
					.formTourSearchItem__img
						img.object-cover(src="./assets/destinations/5.jpg")
					.formTourSearchItem__content
						h4 Langmandi -Beca Room, Pretty House in Centre Hanoi
						.formTourSearchItem__description
							p 09/08 - 11/08
							p Duration: 2 nights
							.tourNewsItem__rating.rating__bar
								.star
								.star__color(data-rating='0.8')
					.formTourSearchItem__priceWrapper
						p From
						p.formTourSearchItem__price 235.888 đ
						p per night
					.formTourSearchItem__action
						span.fas.fa-chevron-down
					.formTourSearchItem__detail
						.checkInOut.formTourSearchItem__detailCol
							h5 Check in
							p.dateChoose__display 15/07/2020
							label.dateChoose
								input.dateChoose__input(disabled)
								span.fal.fa-calendar-alt
